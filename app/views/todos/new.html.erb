<h1>New Todo</h1>
<nav>
<a href='/'>Home</a>
<a href='/todos'>My Todos</a>
</nav>

<%= form_for :todo, url: todos_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', todos_path %>